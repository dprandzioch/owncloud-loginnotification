<?xml version="1.0" encoding="UTF-8" ?>
<database>
    <name>*dbname*</name>
    <create>true</create>
    <overwrite>true</overwrite>
    <charset>utf8</charset>
    <table>
        <name>*dbprefix*loginnotification_items</name>
        <declaration>
            <field>
                <name>id</name>
                <type>integer</type>
                <default>0</default>
                <notnull>true</notnull>
                <autoincrement>1</autoincrement>
                <length>4</length>
            </field>
            <field>
                <name>uid</name>
                <type>text</type>
                <notnull>true</notnull>
                <length>255</length>
            </field>
            <field>
                <name>remote_addr</name>
                <type>text</type>
                <notnull>true</notnull>
                <length>255</length>
            </field>
            <field>
                <name>created_at</name>
                <type>datetime</type>
                <notnull>true</notnull>
                <length>255</length>
            </field>
        </declaration>
    </table>
</database>